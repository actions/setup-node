{
    "problemMatcher": [
        {
            "owner": "tsc",
            "pattern": [
                {
                    "regexp": "/^(.+):\\sline\\s(\\d+),\\scol\\s(\\d+),\\s(Error|Warning|Info)\\s-\\s(.+)\\s\\((.+)\\)$/",
                    "file": 1,
                    "line": 2,
                    "character": 3,
                    "severity": 4,
                    "message": 5,
                    "code": 6
                }
            ]
        }
    ]
}